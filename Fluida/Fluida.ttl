@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .
@prefix state:   <http://lv2plug.in/ns/ext/state#> .
@prefix fluida:  <https://github.com/brummer10/Fluida.lv2#>  .
@prefix mod: <http://moddevices.com/ns/mod#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://github.com/brummer10/Fluida.lv2#me>
    a foaf:Person ;
    foaf:name "brummer" ;
    rdfs:seeAlso <https://github.com/brummer10> .

fluida:soundfont
    a lv2:Parameter ;
    mod:fileTypes "sf2" ;
    rdfs:label "soundfont" ;
    rdfs:range atom:Path .

fluida:reverb_level
    a lv2:Parameter ;
    rdfs:label "Reverb Level" ;
    rdfs:range atom:Float ;
    lv2:default 0.7 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 .

fluida:reverb_width
    a lv2:Parameter ;
    rdfs:label "Reverb Width" ;
    rdfs:range atom:Float ;
    lv2:default 10.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 100.0 .

fluida:reverb_damp
    a lv2:Parameter ;
    rdfs:label "Reverb Damp" ;
    rdfs:range atom:Float ;
    lv2:default 0.4 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 .

fluida:reverb_size
    a lv2:Parameter ;
    rdfs:label "Reverb Roomsize" ;
    rdfs:range atom:Float ;
    lv2:default 0.6 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.2 .

fluida:reverb_on
    a lv2:Parameter ;
    rdfs:label "Reverb On" ;
    rdfs:range atom:Bool .

fluida:chorus_type
    a lv2:Parameter ;
    rdfs:label "Chorus Type" ;
    rdfs:range atom:Int ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 .

fluida:chorus_depth
    a lv2:Parameter ;
    rdfs:label "Chorus Depth" ;
    rdfs:range atom:Float ;
    lv2:default 3.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 21.0 .

fluida:chorus_speed
    a lv2:Parameter ;
    rdfs:label "Chorus speed" ;
    rdfs:range atom:Float ;
    lv2:default 0.3 ;
    lv2:minimum 0.1 ;
    lv2:maximum 5.0 .

fluida:chorus_lev
    a lv2:Parameter ;
    rdfs:label "Chorus Level" ;
    rdfs:range atom:Float ;
    lv2:default 3.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 .

fluida:chorus_voices
    a lv2:Parameter ;
    rdfs:label "Chorus Voices" ;
    rdfs:range atom:Int ;
    lv2:default 3 ;
    lv2:minimum 0 ;
    lv2:maximum 99 .

fluida:chorus_on
    a lv2:Parameter ;
    rdfs:label "Chorus On" ;
    rdfs:range atom:Bool .

fluida:channel_pressure
    a lv2:Parameter ;
    rdfs:label "Channel Pressure" ;
    rdfs:range atom:Int ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 127 .

<https://github.com/brummer10/Fluida.lv2>
    a lv2:Plugin ,
        lv2:InstrumentPlugin ;
    doap:maintainer  <https://github.com/brummer10/Fluida.lv2#me> ;
    doap:name "Fluida";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <https://github.com/brummer10/Fluida.lv2> ;
    lv2:requiredFeature urid:map ;
    lv2:optionalFeature lv2:hardRTCapable ,
                            work:schedule  ,
                            state:loadDefaultState ;
    lv2:extensionData work:interface ,
                    state:interface ;

    guiext:ui <https://github.com/brummer10/Fluida_gui>;

    lv2:minorVersion 1;
    lv2:microVersion 0;

    mod:brand "Synth" ;
    mod:label "Fluida" ;

rdfs:comment """
Fluidsynth as LV2 plugin
""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out" ;
    ]       , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out1" ;
        lv2:name "Out1" ;
    ]      , [
        a lv2:InputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ,
             patch:Message ;
        lv2:designation lv2:control ;
        lv2:index 2 ;
        lv2:symbol "MIDI_IN" ;
        lv2:name "MIDI_IN" ;
    ]      , [
        a lv2:OutputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports patch:Message ;
        lv2:designation lv2:control ;
        lv2:index 3 ;
        lv2:symbol "NOTIFY" ;
        lv2:name "NOTIFY";
    ] ;

    patch:writable fluida:soundfont ,
                fluida:reverb_level ,
                fluida:reverb_width ,
                fluida:reverb_damp ,
                fluida:reverb_size ,
                fluida:reverb_on ,
                fluida:chorus_type ,
                fluida:chorus_depth ,
                fluida:chorus_speed ,
                fluida:chorus_lev ,
                fluida:chorus_voices ,
                fluida:chorus_on ,
                fluida:channel_pressure ;

    patch:readable fluida:reverb_level ,
                fluida:reverb_width ,
                fluida:reverb_damp ,
                fluida:reverb_size ,
                fluida:reverb_on ,
                fluida:chorus_type ,
                fluida:chorus_depth ,
                fluida:chorus_speed ,
                fluida:chorus_lev ,
                fluida:chorus_voices ,
                fluida:chorus_on ,
                fluida:channel_pressure ;

   	state:state [
                fluida:reverb_on 0 ;
                fluida:chorus_on 0
                ] .

<https://github.com/brummer10/Fluida_gui>
  a guiext:X11UI;
  guiext:binary <Fluida_ui.so>;
        lv2:extensionData guiext:idle ;
        lv2:extensionData guiext:resize ;
        lv2:extensionData guiext:idleInterface;
        lv2:requiredFeature guiext:idleInterface ;
        guiext:portNotification [
            guiext:plugin  <https://github.com/brummer10/Fluida.lv2> ;
            lv2:symbol "NOTIFY" ;
            guiext:notifyType atom:Blank
        ] .
    
